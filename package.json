{
  "name": "pothos-drizzle-generator",
  "version": "0.1.14",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p ./tsconfig.esm.json && cpy esm dist",
    "watch": "tsc -b -w",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test:actions": "pnpm run test:reset && pnpm run test:migrate && vitest run",
    "test:watch": "pnpm run test:reset && pnpm run test:migrate && vitest",
    "test:docker": "docker compose -f test/docker/docker-compose.yml up -d",
    "test:generate": "drizzle-kit generate --config test/drizzle.config.ts",
    "test:migrate": "drizzle-kit migrate --config test/drizzle.config.ts",
    "test:seed": "tsx test/tools/seed",
    "test:reset": "tsx test/tools/reset && pnpm run test:migrate",
    "explorer": "tsx --watch test/tools/explorer"
  },
  "dependencies": {
    "@graphql-tools/utils": "^10.11.0",
    "@pothos/core": "^4.10.0",
    "@pothos/plugin-drizzle": "^0.16.1",
    "drizzle-orm": "1.0.0-beta.5-0a7601e",
    "graphql": "^16.12.0",
    "graphql-scalars": "^1.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@hono/graphql-server": "^0.7.0",
    "@hono/node-server": "1.19.7",
    "@pothos/core": "^4.10.0",
    "@pothos/plugin-drizzle": "0.16.1",
    "@types/graphql": "^14.5.0",
    "@urql/core": "6.0.1",
    "apollo-explorer": "1.1.3",
    "cpy-cli": "^6.0.0",
    "dotenv": "17.2.3",
    "drizzle-kit": "1.0.0-beta.5-0a7601e",
    "drizzle-seed": "1.0.0-beta.5-0a7601e",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "graphql-auto-query": "1.0.4",
    "hono": "^4.11.1",
    "jose": "6.1.3",
    "tsx": "4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vitest": "4.0.16"
  },
  "repository": "https://github.com/node-libraries/pothos-drizzle-generator",
  "author": "SoraKumo <info@croud.jp>",
  "license": "MIT"
}
