{
  "name": "pothos-drizzle-generator-plugin",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "dev:docker": "docker compose -f docker/development/docker-compose.yml up -d",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:reset": "tsx tools/reset",
    "drizzle:seed": "tsx tools/seed",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src"
  },
  "dependencies": {
    "@graphql-tools/utils": "10.11.0",
    "@hono/graphql-server": "0.7.0",
    "@hono/node-server": "1.19.7",
    "@pothos/core": "4.10.0",
    "@pothos/plugin-drizzle": "0.15.0",
    "@pothos/plugin-relay": "4.6.2",
    "@types/graphql": "14.5.0",
    "apollo-explorer": "1.1.3",
    "dotenv": "17.2.3",
    "drizzle-orm": "1.0.0-beta.2-278d7e6",
    "graphql": "16.12.0",
    "graphql-auto-query": "1.0.4",
    "graphql-scalars": "1.25.0",
    "graphql-yoga": "5.17.1",
    "hono": "4.10.8",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/pg": "8.16.0",
    "drizzle-kit": "1.0.0-beta.2-278d7e6",
    "drizzle-seed": "1.0.0-beta.2-278d7e6",
    "eslint": "9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "tsx": "4.21.0",
    "typescript-eslint": "^8.49.0"
  },
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/node-libraries/pothos-drizzle-generator",
